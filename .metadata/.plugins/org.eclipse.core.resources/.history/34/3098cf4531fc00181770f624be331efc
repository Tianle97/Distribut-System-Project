<xsd:schema attributeFormDefault="unqualified" 
		    xmlns:gmit="http://Model"
		    targetNamespace="" 
		    elementFormDefault="qualified"
		    xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<xsd:element name='comment' type='xsd:string'/>	
	
	<xsd:simpleType name="bid">
		<xsd:restriction base="xsd:int">
		<!-- used in regular expressions -->
			<xsd:pattern value="[0-9]{5}(-[A-Z]{4})?" />
		</xsd:restriction>
	</xsd:simpleType>
  	
  	<!-- Customer Schema -->
	<xsd:complexType name='Customer'>
		<xsd:annotation>
			<xsd:documentation>
				Customer Schema
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name='cid' type='xsd:int' />
			<xsd:element name='name' type='xsd:string' />
			<xsd:element name='gender' type='xsd:string' />
			<xsd:element name='cunty' type='xsd:string' />
			<xsd:element name='address' type='xsd:string' />
		</xsd:sequence>
	</xsd:complexType>
	
	<!-- Vehicle Schema -->
	<xsd:complexType name='Vehicle'>
		<xsd:annotation>
			<xsd:documentation>
				Vehicle Schema
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name='vid' type='xsd:int' />
			<xsd:element name='carBrand' type='xsd:string' />
			<xsd:element name='year' type='xsd:string' />
			<xsd:element name='color' type='xsd:string' />
		</xsd:sequence>
	</xsd:complexType>
	
	<!-- Booking Schema -->
	<xsd:complexType name='Booking'>
		<xsd:annotation>
			<xsd:documentation>
				Booking Schema
			</xsd:documentation>
		</xsd:annotation>
			<xsd:attribute name='vid' type='xsd:int'/>
		    <xsd:attribute name='cid' type='xsd:int'/>
		    <xsd:attribute name='bid' type='gmit:bid'/>
		    <xsd:attribute name='startDate' type='xsd:string' use='required'/>
		    <xsd:attribute name='endDate' type='xsd:string' use='required'/>
	</xsd:complexType>
	
</xsd:schema>